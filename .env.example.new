# Environment variables example file
# Copy this to .env and fill in your values

# Application Configuration
REACT_APP_NAME=MIT Student Portal
REACT_APP_VERSION=1.0.0

# API Configuration (for future use)
REACT_APP_API_URL=https://student.mitapps.in
REACT_APP_API_KEY=your_api_key_here

# MIT University Configuration
REACT_APP_UNIVERSITY_NAME=MIT-ADT University
REACT_APP_UNIVERSITY_LOGO=https://student.mitapps.in/site/static/images/mit.jpg

# Authentication (for future API integration)
REACT_APP_JWT_SECRET=your_jwt_secret_here

# Deployment
REACT_APP_DEPLOYMENT_URL=https://mit-student-portal.vercel.app
